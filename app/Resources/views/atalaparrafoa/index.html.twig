{% extends 'base.html.twig' %}
{% block menua %}
    {{ parent() }}
    <ul class="list-inline pull-right" role="tablist">
        <li><a href="{{ path('atalaparrafoa_new') }}" class="btn btn-primary">Parrafo berria</a></li>
    </ul>
{% endblock %}
{% block body %}
    <h1>Atalaparrafoa list</h1>

    <table>
        <thead>
            <tr>
                <th>Ordena</th>
                <th>Testuaeu</th>
                <th>Testuaes</th>
                <th>Createdat</th>
                <th>Updatedat</th>
                <th>Id</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for atalaparrafoa in atalaparrafoas %}
            <tr>
                <td><a href="{{ path('atalaparrafoa_show', { 'id': atalaparrafoa.id }) }}">{{ atalaparrafoa.ordena }}</a></td>
                <td>{{ atalaparrafoa.testuaeu }}</td>
                <td>{{ atalaparrafoa.testuaes }}</td>
                <td>{% if atalaparrafoa.createdAt %}{{ atalaparrafoa.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if atalaparrafoa.updatedAt %}{{ atalaparrafoa.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ atalaparrafoa.id }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('atalaparrafoa_show', { 'id': atalaparrafoa.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('atalaparrafoa_edit', { 'id': atalaparrafoa.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('atalaparrafoa_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
