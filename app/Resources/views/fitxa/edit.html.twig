            <div class="col-sm-6">
                <div class="page-header">
                    <h2>{{ labelak.kostualabeleu }}</h2>
                </div>
                {% if eremuak.kostuatext %}
                    <div class="form-group">
                        {{ form_errors(edit_form.kostuaeu) }}
