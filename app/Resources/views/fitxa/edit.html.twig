                            {% if ((eremuak["besteak2text"]==1) or (eremuak["besteak2table"]==1)) %}
                                <li><a href="#besteak2">Besteak2</a></li>
                            {% endif %}

                            {% if ((eremuak["besteak3text"]==1) or (eremuak["besteak3table"]==1)) %}
                                <li><a href="#besteak3">Besteak3</a></li>
                            {% endif %}

                            {% if is_granted('ROLE_ADMIN') %}
