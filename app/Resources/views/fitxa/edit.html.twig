                {% endif %}

                {% if eremuak.ebazpensinpli %}
                    <div class="form-group">
                        <label class="control-label">{{ labelak.ebazpensinplilabeleu }}</label>
                        {{ form_errors(edit_form.ebazpensinpli) }}
