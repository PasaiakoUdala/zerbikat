                                <li><a href="#familia">Familiak</a></li>
                                <li><a href="#etiketak">Etiketak</a></li>
                                <li><a href="#espedienteKudeaketa">Espediente Kudeaketa</a></li>
                                <li><a href="#admin">Admin</a></li>

                            {% endif %}
                        </ul>
                    </li>
                    <li><a href="#">{% if app.request.getLocale()=="eu" %}
