                        <label class="control-label">{{ labelak.aurreikusilabeleu }}
                            /{{ labelak.aurreikusilabeles }}</label>
                        {{ form_errors(edit_form.aurreikusi) }}
                        {{ form_widget(edit_form.aurreikusi, {'attr': {'class': 'form-control'}}) }}
