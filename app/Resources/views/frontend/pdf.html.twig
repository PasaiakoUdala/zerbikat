                            {% else %}
                                <li>{{ kanal.izenaeu }}</li>
                            {% endif %}
                        {% else %}
                            {% if k.esteka %}
                                <li><a href='{{ kanal.estekaes }}' target="_blank">{{ kanal.izenaes }}</a></li>
                            {% else %}
                                <li>{{ kanal.izenaes }}</li>
