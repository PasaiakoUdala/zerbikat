{% extends 'pdf.html.twig' %}

{% block menua %}
{% endblock %}

{% block body %}
    <table border="1" cellpadding="4">
        <tr>
            <td colspan="12" style="text-align:right">
                {% if fitxa.updatedAt %}
                    {% if app.request.locale == 'eu' %}
                        <span style="font-size: xx-small;">Azken eguneraketa: {{ fitxa.updatedAt|date('Y-m-d H:i:s') }}</span>
                    {% else %}
                        <span style="font-size: xx-small;">Ultima actualizaci√≥n: {{ fitxa.updatedAt|date('Y-m-d H:i:s') }}</span>
                    {% endif %}
                {% endif %}
            </td>
        </tr>
        <tr>
            <th colspan="2">
                <img src="{{ fitxa.udala.logoa }}"/>
                <h6 style="text-align:center">{{ fitxa.udala.izendapenaeu }}</h6>
                <h6 style="text-align:center">{{ fitxa.udala.ifk }}</h6>
            </th>
            <th colspan="2"><h2>{{ fitxa.espedientekodea }}</h2></th>
            <th colspan="8">
                <h4>
                    {% if app.request.locale == 'eu' %}{{ fitxa.deskribapenaeu }}
                    {% else %}{{ fitxa.deskribapenaes }}
                    {% endif %}
                </h4>
            </th>
        </tr>
    </table>

    {% if eremuak.helburuatext %}
        <br/>
        <div>
        <span style="font-size: large; font-weight: bold">
            {% if app.request.locale == 'eu' %}
                {{ labelak.helburualabeleu }}
            {% else %}
                {{ labelak.helburualabeles }}
            {% endif %}
        </span>
            <span style="font-size: small;">
            {% if app.request.locale == 'eu' %}
                {{ fitxa.helburuaeu|raw }}
            {% else %}
                {{ fitxa.helburuaes|raw }}
            {% endif %}
        </span>
        </div>
    {% endif %}

    {% if (eremuak.norkeskatutext or eremuak.norkeskatutable) %}
        <div>
        <span style="font-size: large; font-weight: bold">
            {% if app.request.locale == 'eu' %}{{ labelak.norkeskatulabeleu }}
            {% else %}{{ labelak.norkeskatulabeles }}
            {% endif %}
        </span>
            <span style="font-size: small;">
                {% if eremuak.norkeskatutext %}
                    {% if app.request.locale == 'eu' %}{{ fitxa.norkeu |raw }}
                    {% else %}{{ fitxa.norkes |raw }}
                    {% endif %}
                {% endif %}
                {% if eremuak.norkeskatutable %}
                    <ul>
                    {% for nork in fitxa.norkeskatuak %}
                        {% if app.request.locale == 'eu' %}
                            <li>{{ nork.norkeu }}</li>
                        {% else %}
                            <li>{{ nork.norkes }}</li>
                        {% endif %}
                    {% endfor %}
                </ul>
                {% endif %}
        </span>
        </div>
    {% endif %}

    {% if (eremuak.dokumentazioatext or eremuak.dokumentazioatable) %}
        <div>
        <span style="font-size: large; font-weight: bold">
            {% if app.request.locale == 'eu' %}{{ labelak.dokumentazioalabeleu }}
            {% else %}{{ labelak.dokumentazioalabeles }}
            {% endif %}
        </span>
            <span style="font-size: small;">
                {% if eremuak.dokumentazioatable %}
                    <ul>
                    {% for dokumentua in fitxa.dokumentazioak %}
                        <li>
                            {% if app.request.locale == 'eu' %}
                                {% if dokumentua.estekaeu %}
                                    <a href="{{ dokumentua.estekaeu }}">
                                        {{ dokumentua.kodea }} {{ dokumentua.deskribapenaeu }}
                                    </a>
                                {% else %}
                                    {{ dokumentua.kodea }} {{ dokumentua.deskribapenaeu }}
                                {% endif %}
                            {% else %}
                                {% if dokumentua.estekaes %}
                                    <a href="{{ dokumentua.estekaes }}">
                                        {{ dokumentua.kodea }} {{ dokumentua.deskribapenaes }}
                                    </a>
                                {% else %}
                                    {{ dokumentua.kodea }} {{ dokumentua.deskribapenaes }}
                                {% endif %}
                            {% endif %}
                        </li>
                    {% endfor %}
                </ul>
                {% endif %}
                {% if eremuak.dokumentazioatext %}
                    {% if app.request.locale == 'eu' %}{{ fitxa.dokumentazioaeu|raw }}
                    {% else %}{{ fitxa.dokumentazioaes|raw }}
                    {% endif %}
                {% endif %}
        </span>
        </div>
    {% endif %}

    {% if (eremuak.doklaguntext or eremuak.doklaguntable) %}
        <div>
        <span style="font-size: large; font-weight: bold">
                    {% if app.request.locale == 'eu' %}{{ labelak.doklagunlabeleu }}
                    {% else %}{{ labelak.doklagunlabeles }}
                    {% endif %}

        </span>
            <span style="font-size: small;">
                {% if eremuak.doklaguntext %}
                    {% if app.request.locale == 'eu' %}{{ fitxa.doklaguneu |raw }}
                    {% else %}{{ fitxa.doklagunes |raw }}
                    {% endif %}
                {% endif %}
                {% if eremuak.doklaguntable %}
                    <ul>
                    {% for doklagun in fitxa.doklagunak %}
                        <li>
                        {% if app.request.locale == 'eu' %}
                            {% if doklagun.estekaeu %}
                                <a href="{{ doklagun.estekaeu }}">
                                        {{ doklagun.kodea }} {{ doklagun.deskribapenaeu }}
                                    </a>
                            {% else %}
                                {{ doklagun.kodea }} {{ doklagun.deskribapenaeu }}
                            {% endif %}
                        {% else %}
                            {% if doklagun.estekaes %}
                                <a href="{{ doklagun.estekaes }}">
                                    {{ doklagun.kodea }} {{ doklagun.deskribapenaes }}
                                </a>
                            {% else %}
                                {{ doklagun.kodea }} {{ doklagun.deskribapenaes }}
                            {% endif %}
                        {% endif %}
                        </li>
                    {% endfor %}
                </ul>
                {% endif %}
        </span>
        </div>
    {% endif %}


    {% if (eremuak.kanalatext or eremuak.kanalatable) %}
        <div class="row">
        <span style="font-size: large; font-weight: bold">
                    {% if app.request.locale == 'eu' %}{{ labelak.kanalalabeleu }}
                    {% else %}{{ labelak.kanalalabeles }}
                    {% endif %}
                </span>
            <span style="font-size: small;">
                {% if eremuak.kanalatable %}
                    {% for k in kanalmotak %}
                        {% set aurkitua=0 %}
                        <ul>
                    {% for kanal in fitxa.kanalak if kanal.kanalmota == k %}
                        {% if aurkitua==0 %}
                            <b>{{ k.motaeu }}:</b>
                            {% set aurkitua=1 %}
                        {% endif %}
                        {% if app.request.locale == 'eu' %}
                        {% if kanal.telematikoa %}
                            {% if fitxa.zerbitzua %}
                                <li><a href="{{ fitxa.zerbitzua.erroaeu }}{{ fitxa.udala.kodea }}{{ fitxa.parametroa }}">
                                                {{ kanal.izenaeu }}
                                            </a></li>
                            {% endif %}
                            {% else %}
                            {% if k.esteka %}
                                {#<li><a href='{{ kanal.estekaeu }}' target="_blank">{{ kanal.izenaeu }}</a></li>#}
                                <li>{% if kanal.izenaeu %}
                                            {#<a href="{{ kanal.estekaeu }}" target="_blank">{{ kanal.izenaeu }}</a><br />#}
                                            {% if '@' in kanal.estekaeu and 'maps' not in kanal.estekaeu %}
                                        <a href='mailto:{{ kanal.estekaeu }}'>{{ kanal.izenaeu }}</a><br/>
                                            {% else %}
                                        <a href="{{ kanal.estekaeu }}" target="_blank">{{ kanal.izenaeu }}</a><br/>
                                    {% endif %}
                                    {% endif %}
                                    {% if kanal.eraikina %}{{ kanal.eraikina.izena }}<br/>{% endif %}
                                    {% if kanal.kalea %}{{ kanal.kalea }}{% endif %}{% if kanal.kalezbkia %}&nbsp;{{ kanal.kalezbkia }}, {% endif %}
                                    {% if kanal.postakodea %}{{ kanal.postakodea }} {{ fitxa.udala.izenaeu }}<br/>{% endif %}
                                    {% if kanal.ordutegia %}Ordutegia: {{ kanal.ordutegia }}<br/>{% endif %}
                                    {% if kanal.telefonoa %}{{ kanal.telefonoa }}<br/>{% endif %}
                                    {% if kanal.fax %}Faxa: {{ kanal.fax }}<br/>{% endif %}
                                        </li>
                                {% else %}
                                {#<li>{{ kanal.izenaeu }}</li>#}
                                <li>{% if kanal.izenaeu %}{{ kanal.izenaeu }}<br/>{% endif %}
                                    {% if kanal.kalea %}{{ kanal.kalea }}{% endif %}{% if kanal.kalezbkia %}&nbsp;{{ kanal.kalezbkia }}, {% endif %}
                                    {% if kanal.postakodea %}{{ kanal.postakodea }} {{ fitxa.udala.izenaeu }}<br/>{% endif %}
                                    {% if kanal.ordutegia %}Ordutegia: {{ kanal.ordutegia }}<br/>{% endif %}
                                    {% if kanal.telefonoa %}{{ kanal.telefonoa }}<br/>{% endif %}
                                    {% if kanal.fax %}Faxa: {{ kanal.fax }}<br/>{% endif %}
                                        </li>
                            {% endif %}
                        {% endif %}
                        {% else %}
                        {% if kanal.telematikoa %}
                            {% if fitxa.zerbitzua %}
                                <li><a href="{{ fitxa.zerbitzua.erroaes }}{{ fitxa.udala.kodea }}{{ fitxa.parametroa }}">
                                            {{ kanal.izenaes }}
                                        </a></li>
                            {% endif %}
                            {% else %}
                            {% if k.esteka %}
                                {#<li><a href='{{ kanal.estekaes }}' target="_blank">{{ kanal.izenaes }}</a></li>#}
                                <li>{% if kanal.izenaes %}
                                            {#<a href="{{ kanal.estekaes }}" target="_blank">{{ kanal.izenaes }}</a><br /> #}
                                            {% if '@' in kanal.estekaes and 'maps' not in kanal.estekaes %}
                                        <a href='mailto:{{ kanal.estekaes }}'>{{ kanal.izenaes }}</a><br/>
                                            {% else %}
                                        <a href="{{ kanal.estekaes }}" target="_blank">{{ kanal.izenaes }}</a><br/>
                                    {% endif %}
                                    {% endif %}
                                    {% if kanal.eraikina %}{{ kanal.eraikina.izena }}<br/>{% endif %}
                                    {% if kanal.kalea %}{{ kanal.kalea }}{% endif %}{% if kanal.kalezbkia %}&nbsp;{{ kanal.kalezbkia }}, {% endif %}
                                    {% if kanal.postakodea %}{{ kanal.postakodea }} {{ fitxa.udala.izenaes }}<br/>{% endif %}
                                    {% if kanal.ordutegia %}Ordutegia: {{ kanal.ordutegia }}<br/>{% endif %}
                                    {% if kanal.telefonoa %}{{ kanal.telefonoa }}<br/>{% endif %}
                                    {% if kanal.fax %}Fax: {{ kanal.fax }}<br/>{% endif %}
                                    </li>
                                {% else %}
                                {#<li>{{ kanal.izenaes }}</li>#}
                                <li>{% if kanal.izenaes %}{{ kanal.izenaes }}<br/>{% endif %}
                                    {% if kanal.kalea %}{{ kanal.kalea }}{% endif %}{% if kanal.kalezbkia %}&nbsp;{{ kanal.kalezbkia }}, {% endif %}
                                    {% if kanal.postakodea %}{{ kanal.postakodea }} {{ fitxa.udala.izenaes }}<br/>{% endif %}
                                    {% if kanal.ordutegia %}Ordutegia: {{ kanal.ordutegia }}<br/>{% endif %}
                                    {% if kanal.telefonoa %}{{ kanal.telefonoa }}<br/>{% endif %}
                                    </li>
                            {% endif %}
                        {% endif %}
                    {% endif %}
                    {% endfor %}
                    </ul>
                    {% endfor %}
                {% endif %}
                {% if eremuak.kanalatext %}
                    {% if app.request.locale == 'eu' %}{{ fitxa.kanalaeu |raw }}
                    {% else %}{{ fitxa.kanalaes |raw }}
                    {% endif %}
                {% endif %}
        </span>
        </div>
    {% endif %}

    {% if (eremuak.kostuatext or eremuak.kostuatable) %}
        <div>
        <span style="font-size: large; font-weight: bold">
                    {% if app.request.locale == 'eu' %}{{ labelak.kostualabeleu }}
                    {% else %}{{ labelak.kostualabeles }}
                    {% endif %}
        </span>
            <span style="font-size: small;">
                <br/>
                {% set kont=0 %}
                {% if eremuak.kostuatable %}
                    {% if fitxa.udala.zergaor %}
                        {% for kostutaula in kostuZerrenda %}
                            <br/><br/>
                            <table border="1" width="90%" cellpadding="6">
                                <tr>
                                    <th colspan="12" class="text-center" style="font-size: small;"><b>
                                        {% if app.request.locale == 'eu' %}
                                            {{ kostutaula['kodea_prod'] }} - {{ kostutaula['izenburuaeu_prod'] }}
                                        {% else %}
                                            {{ kostutaula['kodea_prod'] }} - {{ kostutaula['izenburuaes_prod'] }}
                                        {% endif %}
                                    </b></th>
                                </tr>
                                {% for parrafo in kostutaula['parrafoak'] %}
                                    <tr>
                                        <td colspan="12" style="font-size: small;">
                                            {% if app.request.locale == 'eu' %}
                                                {{ parrafo['testuaeu']| raw }}
                                            {% else %}
                                                {{ parrafo['testuaes']| raw }}
                                            {% endif %}
                                        </td>
                                    </tr>
                                {% endfor %}
                                {% for kontzeptu in kostutaula['kontzeptuak'] %}
                                    <tr>
                                        <td colspan="9" style="font-size: small;">
                                            {% if app.request.locale == 'eu' %}
                                                {{ kontzeptu['kontzeptuaeu_prod'] }}
                                            {% else %}
                                                {{ kontzeptu['kontzeptuaes_prod'] }}
                                            {% endif %}
                                        </td>
                                        <td NOWRAP colspan="3" style="font-size: small;">
                                            {{ kontzeptu['kopurua_prod'] }} {{ kontzeptu['unitatea_prod'] }}
                                        </td>
                                    </tr>
                                {% endfor %}
                            </table>
                            {% set kont=kont+1 %}
                        {% endfor %}
                    {% else %}
                        {% for azpiatal in fitxa.azpiatalak %}
                            <br/>
                            <table border="1" width="90%" cellpadding="6">
                                <tr>
                                    <th colspan="12">
                                        <b>{{ azpiatal.kodea }}-{{ azpiatal.izenburuaeu }}</b>
                                    </th>
                                </tr>
                                {% for parrafo in azpiatal.parrafoak %}
                                    <tr>
                                        <td colspan="12">{{ parrafo.testua }} </td>
                                    </tr>
                                {% endfor %}

                                {% for kontzeptu in azpiatal.kontzeptuak %}
                                    <tr>
                                        <td colspan="9" style="font-size: xx-small;">
                                            {% if app.request.locale == 'eu' %}{{ kontzeptu.kontzeptuaeu }}
                                                {% if kontzeptu.baldintza %}
                                                    ({{ kontzeptu.baldintza.baldintzaeu }})
                                                {% endif %}
                                            {% else %}{{ kontzeptu.kontzeptuaes }}
                                                {% if kontzeptu.baldintza %}
                                                    ({{ kontzeptu.baldintza.baldintzaes }})
                                                {% endif %}
                                            {% endif %}
                                        </td>
                                        <td colspan="3" style="font-size: xx-small;">
                                            {{ kontzeptu.kopurua }} {{ kontzeptu.unitatea }}
                                        </td>

                                    </tr>
                                {% endfor %}
                            </table><br/>
                            {% set kont=kont+1 %}
                        {% endfor %}
                    {% endif %}
                {% endif %}
                {% if eremuak.kostuatext %}
                    {% if app.request.locale == 'eu' %}
                        {{ fitxa.kostuaeu |raw }}
                    {% else %}
                        {{ fitxa.kostuaes |raw }}
                    {% endif %}
                {% endif %}

                {% if (fitxa.kostuaeu is empty and kont==0 and app.request.locale == 'eu') %}
                    {{ labelak.doanlabeleu }}
                {% endif %}
                {% if (fitxa.kostuaes is empty and kont==0 and app.request.locale == 'es') %}
                    {{ labelak.doanlabeles }}
                {% endif %}
        </span>
        </div>
    {% endif %}

    {% if (eremuak.ebazpensinpli or eremuak.arduraaitorpena or eremuak.aurreikusi or eremuak.arrunta or eremuak.isiltasunadmin) %}
        <div>
        <span style="font-size: large; font-weight: bold">
                    {% if app.request.locale == 'eu' %}{{ labelak.epealabeleu }}
                    {% else %}{{ labelak.epealabeles }}
                    {% endif %}
        </span>
            <div style="font-size: small;">
                {% if eremuak.aurreikusi %}
                    {% if fitxa.aurreikusi %}
                        {% if app.request.locale == 'eu' %}{{ labelak.aurreikusilabeleu }}: {{ fitxa.aurreikusi.epeaeu }}
                        {% else %}{{ labelak.aurreikusilabeles }}: {{ fitxa.aurreikusi.epeaes }}
                        {% endif %}
                    {% endif %}<br>
                {% endif %}
                {% if eremuak.arrunta %}
                    {% if fitxa.arrunta %}
                        {% if app.request.locale == 'eu' %}{{ labelak.arruntalabeleu }}: {{ fitxa.arrunta.epeaeu }}
                        {% else %}{{ labelak.arruntalabeles }}: {{ fitxa.arrunta.epeaes }}
                        {% endif %}
                    {% endif %}<br>
                {% endif %}
                {% if eremuak.ebazpensinpli %}
                    {% if fitxa.ebazpensinpli %}
                        {% if app.request.locale == 'eu' %}{{ labelak.ebazpensinplilabeleu }} <br>
                        {% else %}{{ labelak.ebazpensinplilabeles }} <br>
                        {% endif %}
                    {% endif %}
                {% endif %}
                {% if eremuak.arduraaitorpena %}
                    {% if fitxa.arduraaitorpena %}
                        {% if app.request.locale == 'eu' %}{{ labelak.arduraaitorpenalabeleu }}<br>
                        {% else %}{{ labelak.arduraaitorpenalabeles }}<br>
                        {% endif %}
                    {% endif %}
                {% endif %}
                {% if eremuak.isiltasunadmin %}
                    {% if fitxa.isiltasunadmin %}
                        {% if app.request.locale == 'eu' %}{{ labelak.isiltasunadminlabeleu }}: {{ fitxa.isiltasunadmin.isiltasuneu }}
                        {% else %}{{ labelak.isiltasunadminlabeles }}: {{ fitxa.isiltasunadmin.isiltasunes }}
                        {% endif %}
                    {% endif %}
                {% endif %}
            </div>
        </div>
    {% endif %}

    {% if (eremuak.araudiatext or eremuak.araudiatable) %}
        <div>
        <span style="font-size: large; font-weight: bold">
                    {% if app.request.locale == 'eu' %}{{ labelak.araudialabeleu }}
                    {% else %}{{ labelak.araudialabeles }}
                    {% endif %}
        </span>
            {% set kont=0 %}
            <span style="font-size: small;">
            {% if eremuak.araudiatext %}
                {% if app.request.locale == 'eu' %}{{ fitxa.araudiaeu |raw }}
                {% else %}{{ fitxa.araudiaes |raw }}
                {% endif %}
                <br>
            {% endif %}
                {% if eremuak.araudiatable %}
                    <ul>
                {% for araua in fitxa.araudiak %}
                    <li>
                        {% if app.request.locale == 'eu' %}
                            {#<a href="{{ araua.araudia.estekaeu }}">{{ araua.araudia.arauaeu }}</a> {{ araua.atalaeu }}#}
                            {% if araua.araudia.estekaeu %}
                            <a href="{{ araua.araudia.estekaeu }}">
                                {{ araua.araudia.arauaeu }} </a>{{ araua.atalaeu }}
                            {% else %}
                                {{ araua.araudia.arauaeu }} - {{ araua.atalaeu }}
                            {% endif %}
                        {% else %}
                            {#<a href="{{ araua.araudia.estekaes }}">{{ araua.araudia.arauaes }}</a> {{ araua.atalaes }}#}
                            {% if araua.araudia.estekaes %}
                            <a href="{{ araua.araudia.estekaes }}">
                                {{ araua.araudia.arauaes }}</a>{{ araua.atalaes }}
                            {% else %}
                                {{ araua.araudia.arauaes }} - {{ araua.atalaes }}
                            {% endif %}
                        {% endif %}
                    </li>
                    {% set kont=kont+1 %}
                {% endfor %}
            </ul>
                {% endif %}
                {% if app.request.locale == 'eu' %}
                    {% if (fitxa.araudiaeu is empty and kont==0) %}
                        Ez dagokio
                    {% endif %}
                {% else %}
                    {% if (fitxa.araudiaes is empty and kont==0) %}
                        No corresponde
                    {% endif %}
                {% endif %}
        </span>
        </div>
    {% endif %}

    {% if (eremuak.prozeduratext or eremuak.prozeduratable) %}
        <div>
        <span style="font-size: large; font-weight: bold">
                    {% if app.request.locale == 'eu' %}{{ labelak.prozeduralabeleu }}
                    {% else %}{{ labelak.prozeduralabeles }}
                    {% endif %}
        </span>
            <span style="font-size: small;"><br>
                {% if eremuak.prozeduratext %}
                    {% if app.request.locale == 'eu' %}{{ fitxa.prozeduraeu |raw }}
                    {% else %}{{ fitxa.prozeduraes |raw }}
                    {% endif %}
                {% endif %}
                {% if eremuak.prozeduratable %}
                    <ul>
                    {% for prozedura in fitxa.prozedurak %}
                        <li>
                        {% if app.request.locale == 'eu' %}{{ prozedura.prozedura.prozeduraeu }}
                        {% else %}{{ prozedura.prozedura.prozeduraes }}
                        {% endif %}
                        </li>
                    {% endfor %}
                </ul>
                {% endif %}
        </span>
        </div>
    {% endif %}


    {% if (eremuak.norkebatzitext or eremuak.norkebatzitable) %}
        <div>
        <span style="font-size: large; font-weight: bold">
                    {% if app.request.locale == 'eu' %}{{ labelak.norkebatzilabeleu }}
                    {% else %}{{ labelak.norkebatzilabeles }}
                    {% endif %}
        </span>
            <span style="font-size: small;"><br/>
                {% if eremuak.norkebatzitable %}
                    {% if fitxa.norkebatzi %}
                        {% if app.request.locale == 'eu' %}{{ fitxa.norkebatzi.norkeu }}
                        {% else %}{{ fitxa.norkebatzi.norkes }}
                        {% endif %}
                    {% endif %}
                {% endif %}

                {% if eremuak.norkebatzitext %}
                    {% if app.request.locale == 'eu' %}{{ fitxa.norkonartueu|raw }}
                    {% else %}{{ fitxa.norkonartues|raw }}
                    {% endif %}
                {% endif %}

        </span>
        </div>
    {% endif %}

    <div>
        <span style="font-size: large; font-weight: bold">
                    {% if app.request.locale == 'eu' %}{{ labelak.azpisailalabeleu }}
                    {% else %}{{ labelak.azpisailalabeles }}
                    {% endif %}
        </span>
        <span style="font-size: small;"><br/>
            {% if app.request.locale == 'eu' %} {% if fitxa.azpisaila %}{{ fitxa.azpisaila.saila.sailaeu }} - {{ fitxa.azpisaila.azpisailaeu }}{% endif %}
            {% else %}{% if fitxa.azpisaila %}{{ fitxa.azpisaila.saila.sailaes }} - {{ fitxa.azpisaila.azpisailaes }}{% endif %}
            {% endif %}
        </span>
    </div>

    {% if eremuak.oharraktext %}
        <div class="row">
        <span style="font-size: large; font-weight: bold">
                    {% if app.request.locale == 'eu' %}{{ labelak.oharraklabeleu }}
                    {% else %}{{ labelak.oharraklabeles }}
                    {% endif %}
        </span>
            <span style="font-size: small;"><br/>
                {% if app.request.locale == 'eu' %}{{ fitxa.oharrakeu |raw }}
                {% else %}{{ fitxa.oharrakes |raw }}
                {% endif %}
        </span>
        </div>
    {% endif %}

    {% if (eremuak.besteak1text or eremuak.besteak1table) %}
        <div>
        <span style="font-size: large; font-weight: bold">
                    {% if app.request.locale == 'eu' %}{{ labelak.besteak1labeleu }}
                    {% else %}{{ labelak.besteak1labeles }}
                    {% endif %}
        </span>
