{% extends 'base.html.twig' %}

{% block body %}
    <h1>Prozedura</h1>

    <table>
        <tbody>
            <tr>
                <th>Prozeduraeu</th>
                <td>{{ prozedura.prozeduraeu }}</td>
            </tr>
            <tr>
                <th>Prozeduraes</th>
                <td>{{ prozedura.prozeduraes }}</td>
            </tr>
            <tr>
