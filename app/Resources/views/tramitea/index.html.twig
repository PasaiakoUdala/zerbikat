        <thead>
            <tr>
                <th>Tramitea</th>
                {#<th>Actions</th>#}
            </tr>
        </thead>
        <tbody>
        {% for tramitea in tramiteas %}
            <tr>
                <td>{{ tramitea.tramiteaeu }}</td>
                 <td><a href="{{ path('tramitea_edit', { 'id': tramitea.id }) }}" class="fa fa-pencil"></a></td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
